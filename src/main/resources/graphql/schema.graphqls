extend schema
  @link(url: "https://specs.apollo.dev/federation/v2.3", import: ["@key", "@shareable"])

type Quote @key(fields: "symbol") {
  id: ID!
  symbol: String!
  price: Float!
  change: Float!
  changePercent: Float!
  volume: Int!
  timestamp: String!
}

type Query {
  quote(symbol: String!): Quote @shareable
  quotes(symbols: [String!]!): [Quote!]! @shareable
}